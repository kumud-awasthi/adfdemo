{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFTest145534"
		},
		"AmazonS3Linkedservice_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS3Linkedservice'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Gen2_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Gen2_AzureDataLakeStorage1'"
		},
		"gen2_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'gen2_AzureDataLakeStorage'"
		},
		"AmazonS3Linkedservice_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAY6IAGZPRABUG6WHY"
		},
		"Gen2_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen21212.dfs.core.windows.net"
		},
		"gen2_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen21212.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFTest145534_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS3Linkedservice_secretAccessKey": {
						"value": "[parameters('AmazonS3Linkedservice_secretAccessKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Gen2_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('Gen2_AzureDataLakeStorage1_accountKey')]"
					},
					"gen2_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('gen2_AzureDataLakeStorage_accountKey')]"
					},
					"AmazonS3Linkedservice_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3Linkedservice_properties_typeProperties_accessKeyId')]"
					},
					"Gen2_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('Gen2_AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"gen2_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('gen2_AzureDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}